<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class PersistentClass table="OurTable">
      <meta attribute="class-description">
         This class contains the locations detail. 
      </meta>
      <id name= "locId" type="int" column= "locId" >
         <generator class="native"/>
      </id>
        <property name="country" column="country" type="string"/>
        <property name="region" column="region" type="string"/>
	<property name="city" column="city" type="string"/>
	<property name="postalCode" column="postalCode" type="string"/>
	<property name="latitude" column="latitude" type="string"/>
	<property name="longitude" column="longitude" type="string"/>
	<property name="areaCode" column="areaCode" type="string"/>
	<property name="metroCode" column="metroCode" type="string"/>
     
   </class>
</hibernate-mapping>